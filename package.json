  "scripts": {
    "next": "next",
    "babel": "babel --presets es2015,stage-0 --ignore node_modules,public",
    "babel-node": "babel-node --presets es2015,stage-0 --ignore node_modules,public",
    "lint": "eslint src server",
    "clean": "rimraf public && rimraf compiled",
    "start": "pm2 reload process.json --only $Name",
    "devfrontend": "npm run clean && cross-env NODE_ENV=development nodemon",
    "devbackend": "npm run clean && cross-env NODE_ENV=development nodemon --inspect",
    "prod-app": "pm2 reload process.json",
    "prod-start": "npm run clean && cross-env NODE_ENV=production && BASE_URL=http://localhost:3000 && npm run build && npm run prod-app",
    "prod-stop": "pm2 kill",
    "prod-logs": "pm2 logs",
    "build:dev": "cross-env NODE_ENV=development webpack --debug --colors --display-error-details --config ./webpack/webpack.config.dev-server.js",
    "build:prod": "cross-env NODE_ENV=production webpack --colors --display-error-details --config ./webpack/webpack.config.prod.js",
    "build:node": "cross-env NODE_ENV=production npm run babel -- ./server -d ./compiled",
    "build": "npm run clean && npm run build:prod && npm run build:node",
    "test": "jest",
    "test:watch": "cross-env NODE_ENV=test npm test -- --watch --no-single-run",
    "test:coverage": "jest --coverage",
    "count": "cloc src/",
    "dbimport": ". ./devOps/db_local_import.sh"
  },
  "jest": {
    "verbose": true,
    "coverageDirectory": "./jest/coverage/",
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/jest/__mocks__/fileMock.js",
      "\\.(css|scss|less)$": "identity-obj-proxy"
    }
  },
